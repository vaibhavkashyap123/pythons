'''
def removal(list):
    unwant=[0,4,5]
    for i in sorted(unwant,reverse=True):
        del list[i]
        return list
l1=[11,22,33,44,55,66,77,88,99,111]
ele=removal(l1)
print("after removal of elements the list  would be {}".format(l1,ele))
