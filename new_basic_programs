def lenght_list(list,n):
    l1=[]
    for i in range(0,len(list)):
        lenght=len(list[i])
        if(lenght>n):
            l1.append(list[i])
    return l1
list=['this','that','There','thus','Here','Hleoo']
n=int(input("enter the size: "))
len_n=lenght_list(list,n)
print(len_n)
#two_list_and_have_(atleast)_one_common_member
def common_subsequnce(l1,l2):
    common=False
    for i in range(0,len(l1)):
        for j in range(0,len(l2)):
            if(l1[i]==l2[j]):
                common=True
                return common


l1=['This','That','Thus','There']
l2=['There','Has','His','Have']
commons=common_subsequnce(l1,l2)
if(commons==True):
    print("There are have atleast one common subsequence")
else:
    print("no common subsequence")

